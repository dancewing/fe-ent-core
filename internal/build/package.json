{
  "name": "@ent-core/build",
  "version": "0.0.1",
  "private": true,
  "description": "Build Toolchain for ent-core",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "gulp --require @esbuild-kit/cjs-loader -f gulpfile.ts",
    "dev": "pnpm run stub",
    "build": "npm run build:module:es && npm run build:module:lib && npm run build:full",
    "build:full": "vite -c vite.full.config.ts build",
    "build:module:es": "vite -c vite.module.es.config.ts build",
    "build:module:lib": "vite -c vite.module.lib.config.ts build",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "dependencies": {
    "@ent-core/build-constants": "^0.0.1",
    "@ent-core/build-utils": "^0.0.1",
    "fe-ent-build": "^1.0.1",
    "@pnpm/logger": "^4.0.0",
    "@rollup/plugin-alias": "5.0.0",
    "@rollup/plugin-commonjs": "24.1.0",
    "@rollup/plugin-image": "3.0.2",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.2",
    "@rollup/plugin-replace": "5.0.2",
    "@rollup/plugin-typescript": "11.1.0",
    "@vitejs/plugin-vue": "4.1.0",
    "@vitejs/plugin-vue-jsx": "3.0.1",
    "chalk": "^5.0.1",
    "components-helper": "^2.1.4",
    "consola": "^2.15.3",
    "esbuild": "0.17.17",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "8.0.0",
    "gulp-clean-css": "4.3.0",
    "gulp-cssimport": "^7.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "5.0.0",
    "gulp-rename": "2.0.0",
    "lodash-es": "^4.17.21",
    "rollup": "3.18.0",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-windicss": "1.8.10",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-purge-icons": "0.9.1",
    "rollup-plugin-visualizer": "5.9.0",
    "rollup-plugin-polyfill-node": "^0.11.0",
    "through2": "3.0.2",
    "ts-morph": "^14.0.0",
    "unocss": "^0.50.6"
  },
  "devDependencies": {
    "@ant-design/colors": "^6.0.0",
    "ant-design-vue": "^3.2.0",
    "@esbuild-kit/cjs-loader": "^2.2.1",
    "@types/through2": "^2.0.38",
    "@pnpm/types": "^8.4.0",
    "unbuild": "1.2.1",
    "vue": "^3.2.37",
    "vite": "4.2.2",
    "tslib": "^2.3.1",
    "typescript": "^4.9.4",
    "pkg-types": "^1.0.2"
  }
}
