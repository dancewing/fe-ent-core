    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "ant-design-vue": "^4.1.0",
    "axios": "^1.5.1",
    "chalk": "^4.1.2",
    "clean-css": "^5.2.1",
    "commander": "^7.2.0",
    "consola": "^2.15.3",
