{
  "name": "fe-ent-vue-scripts",
  "version": "3.0.13",
  "description": "Ent Vue Scripts",
  "main": "dist/index.js",
  "bin": {
    "ent-vue-scripts": "/dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ent-framework/fe-ent-core.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "stub": "pnpm run build",
    "build": "pnpm run clean && pnpm run build:lib && pnpm run build:types",
    "build:lib": "tsc && node copy-template.js",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "lint-staged": "npx lint-staged",
    "test": "vitest",
    "lint": "eslint --max-warnings 0  \"src/**/*.{vue,ts,tsx}\" --fix",
    "prettier": "prettier --write  \"src/**/*.{tsx,ts,css,less,vue}\""
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/types": "~7.21.4",
    "@types/clean-css": "^4.2.5",
    "@types/fs-extra": "^9.0.6",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^26.0.24",
    "@types/jsdom": "^16.2.11",
    "@types/less": "^3.0.2",
    "@types/nunjucks": "^3.1.5",
    "@types/js-yaml": "^4.0.2",
    "@types/marked": "^5.0.0",
    "@types/prismjs": "^1.16.6",
    "@types/parse5": "^5.0.0"
  },
