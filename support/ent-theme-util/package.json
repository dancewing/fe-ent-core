{
  "name": "fe-ent-theme-util",
  "version": "1.3.14",
  "description": "Vite plugin for dynamically changing the theme color of the interface",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "es",
    "CHANGELOG.md",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf es",
    "dev": " tsup src/index.ts --dts --format cjs,esm --watch",
    "dev:client": "tsc -w --p tsconfig.client.json",
    "build": "npm run clean  && tsup src/index.ts --dts --format cjs,esm",
    "build:bundle": "rollup -c --bundleConfigAsCjs",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/ ",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "example:dev": "npm -C example run serve",
    "example:build": "npm -C example run build",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "vite"
  ],
  "author": "dancewing",
  "repository": {
    "type": "git",
    "url": "https://github.com/dancewing/fe-ent-core"
  },
  "bugs": {
    "url": "https://github.com/dancewing/fe-ent-core/issues"
  },
  "homepage": "https://github.com/dancewing/fe-ent-core/tree/main/#readme",
  "peerDependencies": {
    "vite": ">=4.2.2"
  },
  "devDependencies": {
    "@ant-design/colors": "^6.0.0",
    "ant-design-vue": "^3.2.0",
    "@rollup/plugin-node-resolve": "15.0.2",
    "@rollup/plugin-typescript": "11.1.0",
    "@types/chalk": "^2.2.0",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/less": "^3.0.2",
    "@types/node": "^16.11.6",
    "cross-env": "^7.0.3",
    "fs-extra": "^9.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.6",
    "rimraf": "^3.0.2",
    "vite": "4.2.2",
    "vue": "^3.2.0",
    "tslib": "^2.3.1",
    "tsup": "6.7.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "clean-css": "^5.1.2",
    "debug": "^4.3.2",
    "esbuild": "0.17.17",
    "less": "^4.1.1",
    "esbuild-plugin-alias": "^0.1.2"
  }
}
