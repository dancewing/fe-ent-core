{
  "name": "fe-ent-core",
  "version": "3.0.1",
  "author": {
    "name": "jeff qian",
    "email": "qbaojian@163.com",
    "url": "https://github.com/ent-framework/fe-ent-core"
  },
  "files": [
    "dist",
    "es",
    "lib",
    "json",
    "theme",
    "global.d.ts"
  ],
  "module": "es/index.mjs",
  "typings": "es/index.d.ts",
  "unpkg": "dist/index.full.min.js",
  "sideEffects": [
    "dist/*",
    "*.less"
  ],
  "packageManager": "pnpm@8.1.0",
  "workspaces": [
    "docs",
    "play",
    "apps/*",
    "extensions/*",
    "support/*"
  ],
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "pnpm -C play --stream dev",
    "dev:docs": "pnpm run -C docs dev",
    "build": "pnpm build:gulp",
    "build:gulp": "pnpm run -C internal/build start",
    "build:all": "pnpm build:support && pnpm build:lib && pnpm --filter './{extensions,apps}/**' --stream --parallel build",
    "build:support": "pnpm --filter './support/**' --stream build",
    "build:extensions": "pnpm --filter './extensions/**' --stream build",
    "build:apps": "pnpm --filter './apps/**' --stream build",
    "build:internal": "pnpm run -r --parallel stub",
    "build:docs": "pnpm run lib:icongen && pnpm run -C docs --stream build",
    "changelog": "pnpm run -C packages/fe-ent-core --stream changelog",
    "build:play": "pnpm -C play --stream build",
    "build:test": "cross-env vite build --mode test && esno ./build/script/postBuild.ts",
    "build:lib": "pnpm run lib:icongen && pnpm run lib:build:component && pnpm run lib:build:style && pnpm run lib:dtsgen && pnpm run lib:jsongen",
    "lib:docgen": "ent-vue-scripts docgen",
    "lib:gen:doc": "ent-vue-scripts docgen",
    "lib:build:component": "ent-vue-scripts build:component -u",
    "lib:build:style": "cross-env DEBUG=true ent-vue-scripts build:style",
    "lib:icongen": "ent-vue-scripts icongen",
    "lib:dtsgen": "ent-vue-scripts dtsgen",
    "lib:jsongen": "ent-vue-scripts jsongen",
    "build:no-cache": "yarn clean:cache && pnpm run build",
    "clean": "pnpm run -r --parallel clean",
    "clean:all": "pnpm run clean:dist && pnpm run clean",
    "clean:extensions": "pnpm --filter './extensions/**' --stream clean",
    "clean:apps": "pnpm --filter './apps/**' --stream clean",
    "clean:support": "pnpm --filter './support/**' --stream clean",
    "clean:dist": "rimraf dist",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "report": "cross-env REPORT=true npm run build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "update:version": "tsx ./scripts/update-version.ts",
    "lint": "pnpm run -r --parallel lint",
    "lint:fix": "pnpm run lint --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint \"src/**/*.{vue,less,css}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:play": "eslint --max-warnings 0  \"play/**/*.{vue,ts,tsx}\" --fix",
    "lint:support": "eslint --max-warnings 0  \"support/**/*.{vue,ts,tsx}\" --fix",
    "lint:lib": "eslint --max-warnings 0  \"src/**/*.{vue,ts,tsx}\"",
    "test:unit": "jest",
    "test:unit-coverage": "jest --coverage",
    "test:gzip": "npx http-server dist --cors --gzip -c-1",
    "test:br": "npx http-server dist --cors --brotli -c-1",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && pnpm install",
    "prepare": "husky install",
    "prepublish": "pnpm run build:libs",
    "publish2remote": "tsx ./scripts/publish.ts",
    "publish2local": "tsx ./scripts/publish-local.ts",
    "publish2ali": "tsx ./scripts/publish-ali.ts",
    "link:global": "tsx ./scripts/pnpmlink.ts",
    "stub": "pnpm run -r --parallel stub",
    "postinstall": "pnpm stub"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/antd": "^0.12.0",
    "@vue/shared": "^3.4.8",
    "@vueuse/core": "^10.6.1",
    "@vueuse/shared": "^10.6.1",
    "@zxcvbn-ts/core": "^1.2.0",
    "async-validator": "^4.2.5",
    "axios": "^1.6.8",
    "cropperjs": "^1.5.13",
    "crypto-js": "^4.2.0",
    "csstype": "^3.1.3",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.38.1",
    "path-to-regexp": "^6.2.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "qs": "^6.12.0",
    "resize-observer-polyfill": "^1.5.1",
    "sortablejs": "^1.15.0",
    "treemate": "^0.3.11",
    "vue": "^3.4.8",
    "vue-i18n": "^9.11.1",
    "vue-router": "^4.3.0",
    "vue-types": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@ent-build/build": "workspace:*",
    "@pnpm/logger": "4.0.0",
    "@pnpm/types": "^8.4.0",
    "@types/crypto-js": "^4.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/intro.js": "^3.0.2",
    "@types/jest": "^27.0.2",
    "@types/less": "^3.0.3",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^18.18.13",
    "@types/showdown": "^1.9.4",
    "@types/tinycolor2": "^1.4.2",
    "ant-design-vue": "^4.1.0",
    "autoprefixer": "^10.4.0",
    "commitizen": "^4.2.4",
    "consola": "^2.15.3",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "esno": "^0.16.3",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "jest": "^29.5.0",
    "eslint-config-ent": "workspace:*",
    "stylelint-config-ent": "workspace:*",
    "ts-config-ent": "workspace:*",
    "lint-staged": "15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-morph": "13.0.3",
    "tslib": "^2.3.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.2.3"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china",
    "ant-design-vue": "4.1.2",
    "vue": "3.4.8",
    "vite": "5.2.3",
    "gifsicle": "5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ent-framework/fe-ent-core.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ent-framework/fe-ent-core/issues"
  },
  "homepage": "https://github.com/ent-framework/fe-ent-core",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  }
}