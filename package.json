  "packageManager": "pnpm@8.1.0",
  "workspaces": [
    "packages/*",
    "extensions/*",
    "support/*",
    "play"
  ],
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "pnpm -C play --stream dev",
    "gen:version": "tsx ./scripts/gen-version.ts",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 pnpm build:support && pnpm build:lib",
    "build:lib": "pnpm run -C internal/build start",
    "build:uno:css": "pnpm run -C internal/build build:uno:css",
    "build:support": "pnpm --filter './support/**' --stream build",
    "build:extensions": "pnpm --filter './extensions/**' --stream build",
    "build:internal": "pnpm run -r --parallel stub",
    "build:play": "pnpm -C play --stream build",
    "build:test": "cross-env vite build --mode test && esno ./build/script/postBuild.ts",
    "build:no-cache": "yarn clean:cache && pnpm run build",
    "clean": "pnpm run clean:dist && pnpm run clean:ext",
    "clean:all": "pnpm run clean:dist && pnpm run clean:support && pnpm run clean:ext",
    "clean:ext": "pnpm --filter './extensions/**' --stream clean",
    "clean:support": "pnpm --filter './support/**' --stream clean",
    "clean:dist": "rimraf dist",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "report": "cross-env REPORT=true npm run build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "update:version": "tsx ./scripts/update-version.ts",
    "lint:eslint": "eslint --cache --max-warnings 0  \"packages/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"packages/**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "test:unit": "jest",
    "test:unit-coverage": "jest --coverage",
    "test:gzip": "npx http-server dist --cors --gzip -c-1",
    "test:br": "npx http-server dist --cors --brotli -c-1",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && pnpm install",
    "prepare": "husky install",
    "prepublish": "pnpm run build:libs",
    "remote": "tsx ./scripts/publish.ts",
    "gen:icon": "esno ./build/generate/icon/index.ts",
    "stub": "pnpm run -r --parallel stub",
    "postinstall": "pnpm stub && pnpm gen:version"
  },
  "peerDependencies": {
    "vue": "^3.2.0",
    "ant-design-vue": "^3.2.17"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "@ent-core/assets": "workspace:*",
    "@ent-core/components": "workspace:*",
    "@ent-core/directives": "workspace:*",
    "@ent-core/hooks": "workspace:*",
    "@ent-core/layouts": "workspace:*",
    "@ent-core/locales": "workspace:*",
    "@ent-core/logics": "workspace:*",
    "@ent-core/router": "workspace:*",
    "@ent-core/store": "workspace:*",
