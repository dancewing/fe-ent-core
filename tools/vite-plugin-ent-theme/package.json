{
  "name": "vite-plugin-ent-theme",
  "version": "1.2.9",
  "description": "Vite plugin for dynamically changing the theme color of the interface",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./es/client": {
      "require": "./es/client/index.js",
      "import": "./es/client/index.mjs"
    }
  },
  "files": [
    "dist",
    "es",
    "CHANGELOG.md",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf es",
    "dev": " tsup src/index.ts --dts --format cjs,esm --watch",
    "dev:client": "tsc -w --p tsconfig.client.json",
    "build": "npm run clean  && npm run build:bundle && npm run build:types",
    "build:bundle": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/ -p src/",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "example:dev": "npm -C example run serve",
    "example:build": "npm -C example run build",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "vite"
  ],
  "author": "Vben",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vbenjs/vite-plugin-theme"
  },
  "bugs": {
    "url": "https://github.com/vbenjs/vite-plugin-theme/issues"
  },
  "homepage": "https://github.com/vbenjs/vite-plugin-theme/tree/master/#readme",
  "peerDependencies": {
    "vite": ">=2.8.6"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/less": "^3.0.2",
    "@types/node": "^14.17.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@vue/compiler-sfc": "^3.0.11",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "fs-extra": "^9.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.6",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4",
    "vite": "^2.8.6",
    "fe-ent-theme-api": "^1.0.1"
  },
  "dependencies": {
    "@types/node": "^14.17.1",
    "chalk": "^4.1.1",
    "clean-css": "^5.1.2",
    "debug": "^4.3.2",
    "esbuild": "^0.11.23",
    "less": "^4.1.1",
    "esbuild-plugin-alias": "^0.1.2"
  }
}