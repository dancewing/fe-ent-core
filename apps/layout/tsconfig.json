{
  "extends": "../../tsconfig.web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "vue-i18n": ["../../node_modules/vue-i18n"],
      "vue-types": ["../../node_modules/vue-types"],
      "axios": ["../../node_modules/axios"],
      "pinia": ["../../node_modules/pinia"],
      "@vueuse/shared": ["../../node_modules/@vueuse/shared"],
      "ant-design-vue": ["../../node_modules/ant-design-vue"],
      "ant-design-vue/es/*": ["../../node_modules/ant-design-vue/es/*"]
    },
    "types": ["vite/client"]
  },
  "include": ["components/**/*", "hooks/*", "store/*", "types.ts", "init.ts"],
  "exclude": [
    "node_modules",
    "**/node_modules/**",
    "dist",
    "es",
    "lib",
    "json",
    "**/dist",
    "**/__tests__/**/*",
    "**/gulpfile.ts",
    "**/test-helper",
    "**/*.md"
  ]
}
