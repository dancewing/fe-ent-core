{
  "extends": "../../tsconfig.web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "fe-ent-core": ["../../packages/fe-ent-core/index.ts"],
      "fe-ent-core/es/*": ["../../packages/fe-ent-core/*"],
      "vue-i18n": ["../../node_modules/vue-i18n"],
      "vue-types": ["../../node_modules/vue-types"],
      "@vueuse/shared": ["../../node_modules/@vueuse/shared"],
      "ant-design-vue": ["../../node_modules/ant-design-vue"]
    }
  },
  "include": ["components", "../../packages/fe-ent-core/"],
  "exclude": [
    "node_modules",
    "**/node_modules/**",
    "dist",
    "es",
    "lib",
    "json",
    "**/dist",
    "**/__tests__/**/*",
    "**/gulpfile.ts",
    "**/test-helper",
    "**/*.md"
  ]
}
