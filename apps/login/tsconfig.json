{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "ts-config-ent/web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "vue": ["../../node_modules/vue"],
      "vue-types": ["../../node_modules/vue-types"],
      "axios": ["../../node_modules/axios"],
      "pinia": ["../../node_modules/pinia"],
      "naive-ui/es/*": ["../../node_modules/naive-ui/es/*"],
      "@types/qrcode": ["../../node_modules/@types/qrcode"],
      "fe-ent-core": ["../../packages/ent-core/es"],
      "fe-ent-core/es/*": ["../../packages/ent-core/es/*"]
    },
    "types": ["vite/client"]
  },
  "include": ["src"],
  "exclude": [
    "node_modules",
    "dist",
    "es",
    "lib",
    "json",
    "**/dist",
    "**/__tests__/**/*",
    "**/gulpfile.ts",
    "**/test-helper",
    "**/*.md"
  ]
}
