{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "ts-config-ent/web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "vue": ["../../node_modules/vue"],
      "vue-types": ["../../node_modules/vue-types"],
      "axios": ["../../node_modules/axios"],
      "pinia": ["../../node_modules/pinia"],
      "ant-design-vue": ["../../node_modules/ant-design-vue"],
      "ant-design-vue/es/*": ["../../node_modules/ant-design-vue/es/*"],
      "@types/qrcode": ["../../node_modules/@types/qrcode"]
    },
    "types": ["vite/client"]
  },
  "include": ["components", "locales", "index.ts"],
  "exclude": [
    "node_modules",
    "dist",
    "es",
    "lib",
    "json",
    "**/dist",
    "**/__tests__/**/*",
    "**/gulpfile.ts",
    "**/test-helper",
    "**/*.md"
  ]
}
