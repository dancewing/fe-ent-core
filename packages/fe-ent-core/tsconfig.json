{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "ts-config-ent/web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "preserveSymlinks": true,
    "types": ["vite/client", "node", "ts-config-ent/support"],
    "paths": {
      "@ent-core/*": ["./*"]
    }
  },
  "include": [
    "components",
    "directives",
    "hooks",
    "locales",
    "logics",
    "router",
    "store",
    "types",
    "utils",
    "index.ts",
    "global.ts",
    "shims-vue.d.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "es",
    "lib",
    "json"
  ]
}
